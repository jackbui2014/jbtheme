{
  "name": "jbtheme",
  "version": "1.0.0",
  "description": "Simple WordPress theme built based on ReactJs",
  "main": [
    "index.js",
    "admin.js"
  ],
  "scripts": {
    "build": "wp-scripts start src/index.js src/admin.js src/block.js  --output-path=assets/js",
    "build-lib": "wp-scripts start src/lib/*/*.js  --output-path=assets/js/lib",
    "css": "node-sass --output-style compressed ./src/scss/ -wo ./assets/css/",
    "check-engines": "wp-scripts check-engines",
    "check-licenses": "wp-scripts check-licenses",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "start": "npm run build",
    "test:e2e": "wp-scripts test-e2e",
    "test:unit": "wp-scripts test-unit-js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackbui2014/jbtheme.git"
  },
  "keywords": [
    "WordPress",
    "ReactJs"
  ],
  "author": "JB Provider",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackbui2014/jbtheme/issues"
  },
  "homepage": "https://github.com/jackbui2014/jbtheme#readme",
  "devDependencies": {
    "@wordpress/scripts": "^16.1.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@wordpress/blocks": "^9.1.7",
    "@wordpress/components": "^14.2.0",
    "@wordpress/element": "^3.2.0",
    "@wordpress/i18n": "^4.2.0",
    "animate.css": "^4.1.1",
    "bootstrap": "^4.6.0",
    "classnames": "^2.3.1",
    "jquery": "^3.6.0",
    "node-sass": "^6.0.1",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-slick": "^0.28.1",
    "scroll-out": "^2.2.12",
    "slick-carousel": "^1.8.1"
  }
}
