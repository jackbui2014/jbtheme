{
	"name": "jbtheme/jbteam",
	"category": "jb-general-blocks",
	"attributes": {
		"id": {
			"source": "attribute",
			"selector": ".jb-team-wrapper",
			"attribute": "id"
		},
		"teams":{
			"source": "query",
			"default": [],
			"selector": ".jb-team-item",
			"query":{
				"image": {
					"source": "attribute",
					"selector": "img.jb-team-item-image",
					"attribute": "src"
				},
				"index": {
					"source": "text",
					"selector": ".jb-team-item-index"
				  },
				"name": {
					"source": "text",
					"type": "string",
					"selector": ".jb-team-item-name",
					"default": ""
				},
				"job_title": {
					"source": "text",
					"selector": ".jb-team-item-job-title",
					"type": "string",
					"default": ""
				},
				"facebook": {
					"source": "attribute",
					"attribute": "href",
					"selector": ".jb-team-item-facebook",
					"type": "string",
					"default": ""
				},
				"twitter": {
					"source": "attribute",
					"attribute": "href",
					"selector": ".jb-team-item-twitter",
					"type": "string",
					"default": ""
				},
				"linkedin": {
					"source": "attribute",
					"attribute": "href",
					"selector": ".jb-team-item-linkedin",
					"type": "string",
					"default": ""
				},
				"google": {
					"source": "attribute",
					"attribute": "href",
					"selector": ".jb-team-item-google",
					"type": "string",
					"default": ""
				}
			}
		},
		"title":{
			"type": "string",
			"selector": ".jb-team-title",
			"default": ""
		},
		"subtitle":{
			"type": "string",
			"selector":".jb-team-subtitle",
			"default": ""
		},
		"background_color": {
			"type": "string",
			"default": "#f9f9f9"

		}
		
	}
}